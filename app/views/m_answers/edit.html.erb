<h1>Editing m_answer</h1>

<% form_for(@m_answer) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :m_question_id %><br />
    <%= f.text_field :m_question_id %>
  </p>
  <p>
    <%= f.label :m_category_id %><br />
    <%= f.text_field :m_category_id %>
  </p>
  <p>
    <%= f.label :body %><br />
    <%= f.text_area :body %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @m_answer %> |
<%= link_to 'Back', m_answers_path %>