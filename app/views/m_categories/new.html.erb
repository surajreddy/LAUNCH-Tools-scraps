<h1>New m_category</h1>

<% form_for(@m_category) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :m_assessment_id %><br />
    <%= f.text_field :m_assessment_id %>
  </p>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', m_categories_path %>