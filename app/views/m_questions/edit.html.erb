<h1>Editing m_question</h1>

<% form_for(@m_question) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :m_assessment_id %><br />
    <%= f.text_field :m_assessment_id %>
  </p>
  <p>
    <%= f.label :body %><br />
    <%= f.text_area :body %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @m_question %> |
<%= link_to 'Back', m_questions_path %>